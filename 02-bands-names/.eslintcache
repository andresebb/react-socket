ioNombre = (event, id) => {\r\n    const nuevoNombre = event.target.value;\r\n\r\n    setBands((bands) =>\r\n      bands.map((band) => {\r\n        if (band.id === id) {\r\n          band.name = nuevoNombre;\r\n        }\r\n\r\n        return band;\r\n      })\r\n    );\r\n  };\r\n\r\n  const onPerdioFoco = (id, nombre) => {\r\n    cambiarNombre(id, nombre);\r\n  };\r\n\r\n  const createRows = () => {\r\n    return bands.map((band) => (\r\n      <tr key={band.id}>\r\n        <td>\r\n          <button className=\"btn btn-primary\" onClick={() => votar(band.id)}>\r\n            +1\r\n          </button>\r\n        </td>\r\n\r\n        <td>\r\n          <input\r\n            className=\"form-control\"\r\n            type=\"text\"\r\n            name=\"\"\r\n            id=\"\"\r\n            value={band.name}\r\n            onChange={(event) => cambioNombre(event, band.id)}\r\n            onBlur={() => onPerdioFoco(band.id, band.name)}\r\n          />\r\n        </td>\r\n        <td> {band.votes}</td>\r\n        <td>\r\n          <button className=\"btn btn-danger\" onClick={() => borrar(band.id)}>\r\n            Borrar\r\n          </button>\r\n        </td>\r\n      </tr>\r\n    ));\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <table className=\"table table-stripped\">\r\n        <thead>\r\n          <tr>\r\n            <th>Vota</th>\r\n            <th>Nombre</th>\r\n            <th>Votos</th>\r\n            <th>Borrar</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>{createRows()}</tbody>\r\n      </table>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default BandList;\r\n",{"ruleId":"23","replacedBy":"24"},{"ruleId":"25","replacedBy":"26"},{"ruleId":"27","severity":1,"message":"28","line":2,"column":8,"nodeType":"29","messageId":"30","endLine":2,"endColumn":15},"no-native-reassign",["31"],"no-negated-in-lhs",["32"],"no-unused-vars","'BandAdd' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]